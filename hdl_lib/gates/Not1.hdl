-- NOT gate built from NAND
-- Not(a) = Nand(a, a)

entity Not1 is
  port(
    a : in bit;
    y : out bit
  );
end entity;

architecture rtl of Not1 is
  component nand2
    port(
      a : in bit;
      b : in bit;
      y : out bit
    );
  end component;
begin
  u_nand: nand2 port map (a => a, b => a, y => y);
end architecture;
